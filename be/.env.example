# MCP Server Configuration
MCP_SERVER_URI=

# AWS Configuration
AWS_REGION=us-east-1
AWS_PROFILE=default

# Bedrock AgentCore Memory (Optional - will be auto-created if not provided)
# Get this ID after first run or from AWS Console
MEMORY_ID=

# AWS Bedrock Prompt Management (Required)
# Get these IDs from AWS Bedrock Console > Prompt Management
# Format: 10-character alphanumeric ID (e.g., CZQ4XBNQ4H)
ROUTER_PROMPT_ID=
WELCOME_PROMPT_ID=
NUMEROLOGY_PROMPT_ID=

# Tarot Agent Prompt IDs
CARD_INTERPRETER_PROMPT_ID=
SPREAD_READER_PROMPT_ID=
LIFE_ADVISOR_PROMPT_ID=

# AWS Prompt Versions (Optional - defaults to DRAFT)
# Specify version numbers after creating prompt versions in AWS
# Leave empty or comment out to use DRAFT version
ROUTER_PROMPT_VERSION=
WELCOME_PROMPT_VERSION=
NUMEROLOGY_PROMPT_VERSION=

# Tarot Prompt Versions
CARD_INTERPRETER_PROMPT_VERSION=
SPREAD_READER_PROMPT_VERSION=
LIFE_ADVISOR_PROMPT_VERSION=

# Bedrock Model Configuration (Optional)
MODEL_ID=amazon.nova-micro-v1:0

# Server Configuration (Optional)
HOST=0.0.0.0
PORT=8000
